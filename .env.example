# 서버 설정
PORT=3000
NODE_ENV=development

# JWT 보안 설정 (필수) - 최소 32자 이상의 강력한 시크릿 키
JWT_SECRET=your-super-secure-jwt-secret-key-here-must-be-at-least-32-characters-long

# 데이터베이스 설정
DATABASE_PATH=./data/game.db

# 게임 설정
DEFAULT_PLAYER_MONEY=50000
DEFAULT_INVENTORY_SIZE=5
TRADE_RADIUS_KM=0.5

# 로그 레벨
LOG_LEVEL=info